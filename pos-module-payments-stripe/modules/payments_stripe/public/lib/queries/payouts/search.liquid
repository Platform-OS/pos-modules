{%- liquid
  assign page = page | default: null
  assign limit = limit | default: null
  assign id = id | default: null
  assign payout_id = payout_id | default: null
  assign currency = currency | default: null
  assign connected_account_id = connected_account_id | default: null
  assign gateway_connected_account_id = gateway_connected_account_id | default: null
  assign state = state | default: null
  assign stripe_account_name = stripe_account_name | default: null

  graphql g = "modules/payments_stripe/payouts/search", page: page, limit: limit, id: id, payout_id: payout_id, currency: currency, connected_account_id: connected_account_id, gateway_connected_account_id: gateway_connected_account_id, state: state, stripe_account_name: stripe_account_name
  if g.records == blank
    log g, type: "QUERY ERROR"
  endif

  return g.records
-%}
